#include<bits/stdc++.h>
using namespace std;

int main(){
	int t,i,l;string s;
	cin>>t;

	while(t--){
		cin>>s;
	
		l=s.size();int v,c;
		v=0;c=0;
		for(i=0;i<l;++i){
	       if(s[i]=='a' or s[i]=='i' or s[i]=='e' or s[i]=='o' or s[i]=='u') ++v;
	       else ++c;
		}
		cout<<v<<" "<<c<<"\n";
	
		
	}
	
	return 0;
}
