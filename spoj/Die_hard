#include<bits/stdc++.h>
using namespace std;
#define T int t;for(scanf("%d",&t);t--;)
int main(){
    int h,a,tim;T{
    scanf("%d%d",&h,&a);
    h+=3;a+=2;tim=1;
    while(1)   {
       if(h<=0 or a<=0) {printf("%d\n",tim-1);break;}
       if(!(tim&1)) {h+=3;a+=2;++tim;}
       else{  if(h>5 and a>10){h-=5;a-=10;++tim;}      //see overall decrement
          else  {h-=20;a+=5;++tim;}       }    } }
    return 0;}
