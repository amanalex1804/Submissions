#include<bits/stdc++.h>
using namespace std;
int A[11][6]; void pr(){A[2][0]=6;A[2][1]=2;A[2][2]=4;A[2][3]=8; A[3][0]=1;A[3][1]=3;A[3][2]=9;A[3][3]=7; A[4][0]=6;A[4][1]=4; A[7][0]=1;A[7][1]=7;A[7][2]=9;A[7][3]=3; A[8][0]=6;A[8][1]=8;A[8][2]=4;A[8][3]=2; A[9][0]=1;A[9][1]=9;}int main(){pr();	int t;cin>>t;while(t--){string s; long int n; cin>>s>>n; int dig=(int)(s[s.size()-1]-'0'); 	if(n==0) cout<<"1";	else if(dig==0 or dig==1 or dig==5 or dig==6 ) cout<<dig; 	else if(dig==2) cout<<A[2][n%4]; else if(dig==3) cout<<A[3][n%4]; else if(dig==4) cout<<A[4][n%2]; else if(dig==7) cout<<A[7][n%4]; else if(dig==8) cout<<A[8][n%4]; else if(dig==9) cout<<A[9][n%2]; cout<<"\n";}return 0;}
