using namespace std;
typedef long long int ll;
int main() {
	int t;cin>>t;
	while(t--){
	    ll n;
	    cin>>n;
	    if(!n) { cout<<n<<"\n"; continue;		}
	    vector<int>v;
	    
	    while(n){
	    	v.push_back(n%3);
	    	n/=3;
	    	
		}
	    for(int i=(int)v.size()-1;i>=0;--i) cout<<v[i];
	    cout<<"\n";
	}
	    
	
	return 0;
}
