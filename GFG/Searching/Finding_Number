#include <bits/stdc++.h>
using namespace std;

int main() {
	
	int t;cin>>t;
	
	while(t--){
	    int n;cin>>n; int A[n+4];
	    for(int i=1;i<=n;++i) cin>>A[i];
	    int x;cin>>x;
	    for(int i=1;i<=n;++i){
	      //  cout<<A[i]<<" "<<x<<endl;
	        if(A[i]==x){
	            cout<<i-1<<"\n";
	            x = -1;
	            break;
	        }
	    }
	    if(x!=-1) cout<<-1<<"\n";
	}
	return 0;
}
