#include<bits/stdc++.h>
using namespace std;
int main()
 {
    ios_base::sync_with_stdio(false); cin.tie(NULL);
	int t;
	cin>>t;

	while(t--){
	    int n;cin>>n;
	    int A[n];
	    for(int i=0;i<n;++i) cin>>A[i];
        int i=0,j=n-1;
        while(i<=j){
            cout<<A[j]<<" ";
            if(i!=j) cout<<A[i]<<" ";
            ++i;--j;
        }
	  //  if(n&1) cout<<A[n/2]<<" ";
	    cout<<"\n";
	
	}
	return 0;
}
