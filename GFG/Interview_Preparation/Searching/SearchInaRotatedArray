#include<bits/stdc++.h>
using namespace std;
int main()
 {
	int t;
	cin>>t;

	while(t--){
	    
	    int n,k;cin>>n;
	    int pos =-1;
	    int A[n+5];
	    for(int i=0;i<n;++i) cin>>A[i];
	    cin>>k;
	    for(int i=0,x;i<n;++i){
	        
	        if(A[i]==k){
	            if(pos==-1) pos=i;
	        }
	    }
	    cout<<pos<<"\n";
	
	}
	return 0;
}
