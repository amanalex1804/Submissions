#include<bits/stdc++.h>
using namespace std;
bool palin(string s,int n){
    for(int i=0;i<n/2;++i){
        if(s[i]!=s[n-i-1]) return false;
    }
    return true;
}
int main()
 {
	int t;
	cin>>t;

	while(t--){
	    int n;cin>>n;
	 string s;cin>>s;
	 if(palin(s,n)) cout<<"Yes\n";
	 else cout<<"No\n";
	}
	return 0;
}
