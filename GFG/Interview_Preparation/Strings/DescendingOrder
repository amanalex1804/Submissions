#include<bits/stdc++.h>
using namespace std;
int main()
 {
	int t;
	cin>>t;

	while(t--){
	 string s;cin>>s;
	 int A[27];
	 memset(A,0,sizeof(A));
	 for(int i=0;i<(int)s.size();++i) A[(int)(s[i]-'a')]+=1;
	 for(int i=25;i>=0;--i){
	     while(A[i]--){
	         cout<<(char)(i+'a');
	     }
	 }
	 cout<<"\n";
	 
	}
	return 0;
}
