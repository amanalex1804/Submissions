#include<bits/stdc++.h>
using namespace std;
int main()
 {
    ios_base::sync_with_stdio(false); cin.tie(NULL);
	int t;
	cin>>t;

	while(t--){
	    long int n;cin>>n;
        int k =2;	    
         int result=0; //for n=1
        for(int i=2;i<=n;i++)
        {
        	result=(k+result)%i;
        }
      cout<<(result+1)<<"\n";  //adding 1 to change indexing;
	    
	
	}
	return 0;
}
