#include<bits/stdc++.h>
using namespace std;
int main()
 {
	int t;
	cin>>t;

	while(t--){
	    long int n;cin>>n;
	    int dig = (int)log2(n);
	    int cnt = 0,pos=-1;
	    for(int i=0;i<=dig;++i){
	        if((1LL<<i)&n){
	            ++cnt;
	           
	        }
	    }
	    
	    cout<<cnt<<"\n";
	}
	return 0;
}
