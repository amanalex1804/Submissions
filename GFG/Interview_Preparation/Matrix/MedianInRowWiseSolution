#include<bits/stdc++.h>
using namespace std;
int main()
 {
	int t;
	cin>>t;

	while(t--){
	    vector<int>v;
	    int n,m;cin>>n>>m;
	    for(int i=0;i<n;++i){
	        for(int j=0,x;j<m;++j){
	            cin>>x;
	            v.push_back(x);
	        }
	    }
	    sort(v.begin(),v.end());
	    cout<<v[(int)v.size()/2]<<"\n";
	
	}
	return 0;
}
