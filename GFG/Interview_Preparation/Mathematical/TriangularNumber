#include <bits/stdc++.h>
using namespace std;
bool fn(long int n){
    for(long int i=1;i<=(n/2)+1;++i){
        if((i*(i+1))==(2*n)){
            return 1;
        }
    }
    return 0;
}
int main() {
	int t;cin>>t;
	while(t--){
	    long int n;cin>>n;
	    cout<<fn(n)<<"\n";
	    
	}
	return 0;
}
