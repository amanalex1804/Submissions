#include <bits/stdc++.h>
using namespace std;

int main() {
	ios_base::sync_with_stdio(false); cin.tie(NULL);
	int t;cin>>t;
	while(t--){
	    int n,k;cin>>n>>k;
	    vector<int>v;
	    for(int i=0,x;i<n;++i)
	    cin>>x,v.push_back(x);
	    
	    sort(v.begin(),v.end(),greater<int>());
	    for(int i=0;i<k;++i) cout<<v[i]<<" ";
	    cout<<"\n";
	}
	return 0;
}
