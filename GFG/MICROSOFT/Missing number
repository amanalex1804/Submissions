#include <bits/stdc++.h>
using namespace std;
typedef long int ll;
int main() {
	ios_base::sync_with_stdio(false); cin.tie(NULL);
	int t;cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    ll sum=(n*(n+1))/2;
	    ll psum=0;
	   
	    for(int i=0,x;i<n-1;++i) cin>>x,psum+=x;
	   
	    cout<<(ll)(sum-psum)<<"\n";
	}
	
	return 0;
}
