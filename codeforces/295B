#include<Bits/stdc++.h>
using namespace std;
typedef long long int ll;
#define INF 99999999LL

int main(){
	int n;
	cin>>n;
	ll mat[n+5][n+5];
	int free[n+5];
	for(int i=1;i<=n;++i)
	for(int j=1;j<=n;++j)
	mat[i][j]=INF;
	for(int i=0;i<=n;++i) mat[i][i]=0;
	
	for(int i=1;i<=n;++i)
	for(int j=1;j<=n;++j)
	cin>>mat[i][j];
	
	for(int i=1;i<=n;++i) cin>>free[i];
	bool vis[n+5];
	memset(vis,0,sizeof(vis));
	vector<ll>ans;
	for(int k1=n;k1>=1;--k1){
		vis[free[k1]]=true;
		ll sum=0;
		
		int j1=free[k1];
		for(int ff=k1+1;ff<=n;++ff) {
			int i1=free[ff];
			for(int kk=k1+1;kk<=n;++kk){
				mat[i1][j1]=min(mat[i1][j1],mat[i1][free[kk]]+mat[free[kk]][j1]);
				mat[j1][i1]=min(mat[j1][i1],mat[j1][free[kk]]+mat[free[kk]][i1]);
			}
		}
		
		int k=k1;
		for(int i=1;i<=n;++i)
		for(int j=1;j<=n;++j)
		if(vis[i] and vis[j]) mat[i][j]=min(mat[i][j],mat[i][free[k]]+mat[free[k]][j]);
		
	
		
		for(int i=1;i<=n;++i)
		for(int j=1;j<=n;++j)
		if(vis[i] and vis[j]) sum+=mat[i][j];
		ans.push_back(sum);
	}
	for(int i=ans.size()-1;i>=0;--i) cout<<ans[i]<<" ";
	
	return 0;
}
