#include<bits/stdc++.h>
using namespace std;
#define ull unsigned long long int
int main()
{
    ull l,r;
    cin>>l>>r;
    if(l==r) cout<<l;
    else{
    int cnt2=0,cnt3=0;
    cnt2=(r/2)-(l/2);
    if((l%2==0 and r%2==0) or (l%2==0 and r%2!=0)) cnt2+=1;
    cnt3=(r/3)-(l/3);
     if((l%3==0 and r%3==0) or (l%3==0 and r%3!=0)) cnt3+=1;
     if(cnt2>cnt3) cout<<"2";
     else cout<<"3\n";}



    return 0;
}
